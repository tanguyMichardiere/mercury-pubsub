{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "format": {
      "outputs": []
    },
    "check-format": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "publish": {
      "dependsOn": ["check-format", "lint", "build", "test"],
      "outputs": []
    }
  }
}
